{
  "functions": {
    "api/**": {
      "includeFiles": "src/**"
    }
  },
  "rewrites": [
    {
      "source": "/users.csv",
      "destination": "/api/users.mjs"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://t.me/SearchingChargeBot"
    },
    {
      "source": "/api/worker(.*)",
      "destination": "https://searching-charge.herokuapp.com"
    }
  ],
  "github": {
    "silent": true
  }
}
